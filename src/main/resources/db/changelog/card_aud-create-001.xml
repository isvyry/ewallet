<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">

    <changeSet  id="1"  author="isvyry">
        <createTable  tableName="card_aud">
            <column  name="id"  type="bigint">
                <constraints  primaryKey="true"  nullable="false"/>
            </column>
            <column  name="rev"  type="integer">
                <constraints  primaryKey="true"  nullable="false"/>
            </column>
            <column  name="revtype"  type="smallint"/>
            <column  name="card_number"  type="varchar(255)"/>
            <column  name="balance"  type="numeric(38,2)"/>
            <column  name="expiration_date"  type="timestamp(6)"/>
            <column  name="wallet_id"  type="bigint"/>
            <column  name="is_deleted"  type="boolean"/>
            <column  name="created_date"  type="timestamp(6)"/>
            <column  name="created_by"  type="varchar(255)"/>
            <column  name="last_modified_date"  type="timestamp(6)"/>
            <column  name="last_modified_by"  type="varchar(255)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
